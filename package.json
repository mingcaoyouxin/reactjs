{
  "name": "mobx-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX, ES6 compilation and hot code reloading",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "npm run test:webpack && npm run test:mocha",
    "test:webpack": "NODE_ENV=production webpack --config webpack/test.config.js",
    "test:mocha": "mocha test/.test-bundle.js --reporter nyan --require test/test_helper.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Michel Weststrate <mweststrate@gmail.com> (http://github.com/mweststrate)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "http://mobxjs.github.com/mobx",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "eslint-plugin-react": "^5.1.1",
    "style-loader": "^0.18.1",
    "webpack": "^1.9.6",
    "css-loader": "^0.28.4",
    "webpack-dev-server": "^1.8.2",
    "babel-eslint": "^6.1.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "file-loader": "^0.11.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "url-loader": "^0.5.8"
  },
  "dependencies": {
    "fuse.js": "^2.2.0",
    "mobx": "^2.0.0",
    "mobx-react": "^3.0.0",
    "mobx-react-devtools": "^4.0.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-select": "^1.0.0-beta13",
    "react-router": "^3.0.5",
    "tinper-bee": "^0.4.3"
  }
}
